{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "jsx-alignment": false,
        "jsx-wrap-multiline": false,
        "no-var-requires": false,
        "trailing-comma": false,
        "typedef-whitespace": false,
        "jsx-no-multiline-js": false,
        "space-before-function-paren" : false
    },
    "rulesDirectory": []
}